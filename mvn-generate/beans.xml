<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person" class="com.WadeCourse.spring.test.Person">
		<constructor-arg value="777" name="id" type="int"></constructor-arg>
		<constructor-arg value="Mary" name="name" type="String"></constructor-arg>
		<property name="testId" value="123"></property>
		<property name="address" ref="address"></property>
	</bean>
	<bean id="address" class="com.WadeCourse.spring.test.Address">
		<constructor-arg name="street" value='"No1 xxx Rd"'></constructor-arg>
		<constructor-arg name="postcode" value="9876"></constructor-arg>
	</bean>
	<bean id="address2" class="com.WadeCourse.spring.test.Address" p:street="No2 yy Rd" p:postcode="6789">
		
	</bean>
</beans>
